<div class="container" ></div>
<div class="col-md-4 mx-auto ">
<div class="container text-center mt-1">
  <div class="mt-3 p-3 bg-dark text-white ">
      <h1>USER GAME BIODATA</h1>
    </div>
</div>
</div>
</div>

<div class="container" ></div> 
<div class="mt-4  bg-dark text-white "></div> 
<div class="table-responsive">
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0" style= "border : 1px solid">
        <thead>
            <tr>
                <th style= "border : 1px solid; text-align: center;">Nomor</th>
                <th style= "border : 1px solid; text-align: center;">Id</th>
                <th style= "border : 1px solid; text-align: center;">Id Pengguna</th>
                <th style= "border : 1px solid; text-align: center;">Nama</th>
                <th style= "border : 1px solid; text-align: center;">Tanggal Lahir</th>
                <th style= "border : 1px solid; text-align: center;">Umur</th>
                <th style= "border : 1px solid; text-align: center;">Jenis Kelamin</th>
            </tr>
        </thead>
        <tbody>
            <% usergamebiodata.forEach((element,index) => { %>
            <tr>
                <td style= "border : 1px solid; text-align: center;"><%= ++index %> </td>
                <td style= "border : 1px solid; text-align: center;"><%= element.id %> </td>
                <td style= "border : 1px solid; text-align: center;"><%= element.id_pengguna %></td>
                <td style= "border : 1px solid; text-align: center;"><%= element.nama %></td>
                <td style= "border : 1px solid; text-align: center;"><%= element.tanggal_lahir %></td>
                <td style= "border : 1px solid; text-align: center;"><%= element.umur %></td>
                <td style= "border : 1px solid; text-align: center;"><%= element.jenis_kelamin %></td>
                
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>
</div>
</div>